<%-include('header.ejs')-%>
		<div class="box" id="heading">
			<h1><%= listTitle%></h1>
		</div>
		<div class="box">
			<% for(var i=0;i<listItems.length;i++){ %>
			<div class="item">
				<input type="checkbox">
				<p><%=listItems[i] %></p> 
			</div>
			<% } %>

			<form class="item" target="/" method="POST">
				<input type="text" name="listItem" required autofocus placeholder="New Item" autocomplete="off">
				<button type="submit" value=<%= listTitle %>>+</button>
			</form>
		</div>
<%-include('footer.ejs')-%>
